using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

/// <summary>
/// Se foloseste la afisarea panoului de administrare.
/// </summary>
public class UserRole
{
    public string Email;
    public bool isAdmin, isUploader, isModerator;

	public UserRole(string Email, bool isAdmin, bool isUploader, bool isModerator)
	{
        this.Email = Email;
        this.isAdmin = isAdmin;
        this.isUploader = isUploader;
        this.isModerator = isModerator;
	}
}