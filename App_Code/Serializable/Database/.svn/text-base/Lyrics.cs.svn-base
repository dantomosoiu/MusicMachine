using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;

/// <summary>
/// Se foloseste la obtinerea versurilor unei melodii.
/// </summary>
public class Lyrics
{
    public string SongName;
    public string Artists;
    public string SongLyrics;

	public Lyrics(string SongName, string Artists, string Lyrics)
	{
        this.SongName = SongName;
        this.Artists = Artists;
        this.SongLyrics = Lyrics;
	}
}